      <command
            defaultHandler="eu.compassresearch.ide.rttmbt.RttMbtDeleteFile"
            id="eu.compassresearch.ide.rttmbt.DeleteLocalAndServer"
            name="Delete RTT-MBT Resource">
      </command>
