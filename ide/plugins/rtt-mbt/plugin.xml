   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="eu.compassresearch.ide.rttmbt.RttMbtTgenAndExecute"
            commandId="eu.compassresearch.ide.rttmbt.RttMbtTgenAndExecute">
         <activeWhen>
            <with
                  variable="eu.compassresearch.ide.rttmbt.IsGenerationContextTP">
               <equals
                     value="truumpel">
               </equals>
            </with>
         </activeWhen>
      </handler>
      <handler
            class="eu.compassresearch.ide.rttmbt.RttMbtCleanTestProcedureGenerationContext"
            commandId="eu.compassresearch.ide.rttmbt.RttMbtCleanTestGenCtx">
         <activeWhen>
            <with
                  variable="eu.compassresearch.ide.rttmbt.IsGenerationContextTP">
               <equals
                     value="truumpel">
               </equals>
            </with>
         </activeWhen>
      </handler>
      <handler
            class="eu.compassresearch.ide.rttmbt.RttMbtGenerateTest"
            commandId="eu.compassresearch.ide.rttmbt.RttMbtGenerateTest">
         <activeWhen>
            <with
                  variable="eu.compassresearch.ide.rttmbt.IsGenerationContextTP">
               <equals
                     value="truumpel">
               </equals>
            </with>
         </activeWhen>
      </handler>
      <handler
            class="eu.compassresearch.ide.rttmbt.RttMbtGenerateSimulation"
            commandId="eu.compassresearch.ide.rttmbt.RttMbtGenerateSimulation">
         <activeWhen>
            <with
                  variable="eu.compassresearch.ide.rttmbt.IsGenerationContextTP">
               <equals
                     value="truumpel">
               </equals>
            </with>
         </activeWhen>
      </handler>
      <handler
            class="eu.compassresearch.ide.rttmbt.RttMbtCompileRunDocReplay"
            commandId="eu.compassresearch.ide.rttmbt.RttMbtCompileRunDocReplay">
         <activeWhen>
            <with
                  variable="eu.compassresearch.ide.rttmbt.IsExecutionContextTP">
               <equals
                     value="truumpel">
               </equals>
            </with>
         </activeWhen>
      </handler>
      <handler
            class="eu.compassresearch.ide.rttmbt.RttMbtCleanRttTestProcedure"
            commandId="eu.compassresearch.ide.rttmbt.RttMbtCleanTest">
         <activeWhen>
            <with
                  variable="eu.compassresearch.ide.rttmbt.IsExecutionContextTP">
               <equals
                     value="truumpel">
               </equals>
            </with>
         </activeWhen>
      </handler>
      <handler
            class="eu.compassresearch.ide.rttmbt.RttMbtCompileRttTestProcedure"
            commandId="eu.compassresearch.ide.rttmbt.RttMbtCompileTest">
         <activeWhen>
            <with
                  variable="eu.compassresearch.ide.rttmbt.IsExecutionContextTP">
               <equals
                     value="truumpel">
               </equals>
            </with>
         </activeWhen>
      </handler>
      <handler
            class="eu.compassresearch.ide.rttmbt.RttMbtRunRttTestProcedure"
            commandId="eu.compassresearch.ide.rttmbt.RttMbtRunTest">
         <activeWhen>
            <with
                  variable="eu.compassresearch.ide.rttmbt.IsExecutionContextTP">
               <equals
                     value="truumpel">
               </equals>
            </with>
         </activeWhen>
      </handler>
      <handler
            class="eu.compassresearch.ide.rttmbt.RttMbtReplayTestprocedure"
            commandId="eu.compassresearch.ide.rttmbt.RttMbtReplayTest">
         <activeWhen>
            <or>
               <with
                     variable="eu.compassresearch.ide.rttmbt.IsGenerationContextTP">
                  <equals
                        value="truumpel">
                  </equals>
               </with>
               <with
                     variable="eu.compassresearch.ide.rttmbt.IsExecutionContextTP">
                  <equals
                        value="truumpel">
                  </equals>
               </with>
            </or>
         </activeWhen>
      </handler>
      <handler
            class="eu.compassresearch.ide.rttmbt.RttMbtDocRttTestProcedure"
            commandId="eu.compassresearch.ide.rttmbt.RttMbtDocTest">
         <activeWhen>
            <with
                  variable="eu.compassresearch.ide.rttmbt.IsExecutionContextTP">
               <equals
                     value="truumpel">
               </equals>
            </with>
         </activeWhen>
      </handler>
      <handler
            class="eu.compassresearch.ide.rttmbt.RttMbtCheckModel"
            commandId="eu.compassresearch.ide.rttmbt.RttMbtCheckModel">
         <activeWhen>
            <with
                  variable="eu.compassresearch.ide.rttmbt.IsModelDumpSelected">
               <equals
                     value="truumpel">
               </equals>
            </with>
         </activeWhen>
      </handler>
      <handler
            class="eu.compassresearch.ide.rttmbt.RttMbtImportModel"
            commandId="eu.compassresearch.ide.rttmbt.RttMbtImportModel">
         <activeWhen>
            <with
                  variable="eu.compassresearch.ide.rttmbt.IsRttMbtProjectSelected">
               <equals
                     value="truumpel">
               </equals>
            </with>
         </activeWhen>
      </handler>
      <handler
            class="eu.compassresearch.ide.rttmbt.RttMbtCleanProject"
            commandId="eu.compassresearch.ide.rttmbt.RttMbtCleanProject">
         <activeWhen>
            <with
                  variable="eu.compassresearch.ide.rttmbt.IsRttMbtProjectSelected">
               <equals
                     value="truumpel">
               </equals>
            </with>
         </activeWhen>
      </handler>
      <handler
            class="eu.compassresearch.ide.rttmbt.RttMbtBuildMakeTarget"
            commandId="eu.compassresearch.ide.rttmbt.RttMbtBuildMakeTarget">
         <activeWhen>
            <with
                  variable="eu.compassresearch.ide.rttmbt.IsMakefileSelected">
               <equals
                     value="truumpel">
               </equals>
            </with>
         </activeWhen>
      </handler>
