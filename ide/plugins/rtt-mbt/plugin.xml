      </command>
      <command
            id="eu.compassresearch.ide.rttmbt.RttMbtGenerateTest"
            name="Generate Test">
      </command>
      <command
            id="eu.compassresearch.ide.rttmbt.RttMbtGenerateSimulation"
            name="Generate Simulation">
      </command>
      <command
            id="eu.compassresearch.ide.rttmbt.RttMbtCompileRunDocReplay"
            name="Execute Test">
      </command>
      <command
            id="eu.compassresearch.ide.rttmbt.RttMbtCleanTest"
