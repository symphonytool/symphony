<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="eu.compassresearch.ide.rttmbt.PerspectiveFactory"
            icon="icons/rtt-mbt-small.png"
            id="eu.compassresearch.ide.rttmbt.rtt-perspective"
            name="RttPerspective">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="eu.compassresearch.ide.rttmbt.rtt-perspective">
         <view
               id="eu.compassresearch.ide.rttmbt.RttMbtProjectExplorer"
               minimized="false"
               ratio="0.25"
               relationship="left"
               relative="org.eclipse.ui.editorss"
               visible="true">
         </view>
         <view
               id="eu.compassresearch.ide.rttmbt.ConsoleView"
               minimized="false"
               ratio="0.80"
               relationship="bottom"
               relative="org.eclipse.ui.editorss"
               visible="true">
         </view>
         <view
               id="eu.compassresearch.ide.rttmbt.ProgressView"
               minimized="false"
               ratio="0.75"
               relationship="right"
               relative="org.eclipse.ui.editorss"
               visible="true">
         </view>
         <view
               id="org.eclipse.ui.views.ContentOutline"
               minimized="false"
               ratio="0.75"
               relationship="top"
               relative="eu.compassresearch.ide.rttmbt.ProgressView"
               visible="true">
         </view>
      </perspectiveExtension>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            category="eu.compassresearch.ide.ui.category"
            class="eu.compassresearch.ide.rttmbt.RttMbtConsoleView"
            icon="icons/rtt-mbt-small.png"
            id="eu.compassresearch.ide.rttmbt.ConsoleView"
            name="Console"
            restorable="true">
      </view>
      <view
            category="eu.compassresearch.ide.ui.category"
            class="eu.compassresearch.ide.rttmbt.RttMbtProgressView"
            icon="icons/rtt-mbt-small.png"
            id="eu.compassresearch.ide.rttmbt.ProgressView"
            name="ProgressView"
            restorable="true">
      </view>
      <view
            allowMultiple="false"
            category="eu.compassresearch.ide.ui.category"
            class="eu.compassresearch.ide.rttmbt.RttMbtProjectExplorer"
            icon="icons/rtt-mbt-small.png"
            id="eu.compassresearch.ide.rttmbt.RttMbtProjectExplorer"
            name="ProjectExplorer"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.navigator.viewer">
      <viewerContentBinding
            viewerId="eu.compassresearch.ide.rttmbt.RttMbtProjectExplorer">
         <includes>
            <contentExtension
                  pattern="org.eclipse.ui.navigator.resourceContent">
            </contentExtension>
            <contentExtension
                  pattern="org.eclipse.ui.navigator.resources.filters.*">
            </contentExtension>
            <contentExtension
                  pattern="org.eclipse.ui.navigator.resources.linkHelper">
            </contentExtension>
            <contentExtension
                  pattern="org.eclipse.ui.navigator.resources.workingSets">
            </contentExtension>
         </includes>
      </viewerContentBinding>
      <viewerActionBinding
            viewerId="eu.compassresearch.ide.rttmbt.RttMbtProjectExplorer">
         <includes>
            <actionExtension
                  pattern="org.eclipse.ui.navigator.resources.*">
            </actionExtension>
         </includes>
      </viewerActionBinding>
   </extension>
   <extension
         point="org.eclipse.ui.newWizards">
      <wizard
            category="compass_eu.compassresearch.ide.ui.category"
            class="eu.compassresearch.ide.rttmbt.RttMbtComponentWizard"
            finalPerspective="eu.compassresearch.ide.rttmbt.rtt-perspective"
            icon="icons/rtt-mbt-small.png"
            id="eu.compassresearch.ide.rttmbt.RttMbtComponentWizard"
            name="RTT-MBT Folder">
      </wizard>
      <wizard
            category="compass_eu.compassresearch.ide.ui.category"
            class="eu.compassresearch.ide.rttmbt.RttMbtProjectWizard"
            finalPerspective="eu.compassresearch.ide.rttmbt.rtt-perspective"
            icon="icons/rtt-mbt-small.png"
            id="eu.compassresearch.ide.rttmbt.RttMbtProjectWizard"
            name="RTT-MBT Project"
            project="true">
      </wizard>
      <category
            id="compass_eu.compassresearch.ide.rttmbt.category"
            name="RT-Tester"
            parentCategory="compass_eu.compassresearch.ide.ui.category">
      </category>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="eu.compassresearch.ide.rttmbt.RttMbtPreferencesPage"
            id="eu.compassresearch.ide.rttmbt.RttMbtPreferencesMainPage"
            name="RT-Tester">
      </page>
      <page
            category="eu.compassresearch.ide.rttmbt.RttMbtPreferencesMainPage"
            class="eu.compassresearch.ide.rttmbt.RttMbtPreferencesServerPage"
            id="eu.compassresearch.ide.rttmbt.RttMbtPreferencesServerPage"
            name="Server">
      </page>
      <page
            category="eu.compassresearch.ide.rttmbt.RttMbtPreferencesMainPage"
            class="eu.compassresearch.ide.rttmbt.RttMbtPreferencesProjectPage"
            id="eu.compassresearch.ide.rttmbt.RttMbtPreferencesProjectPage"
            name="Project">
      </page>
   </extension>
   <extension
         point="org.eclipse.core.expressions.propertyTesters">
      <propertyTester
            class="eu.compassresearch.ide.rttmbt.RttMbtPropertyTester"
            id="eu.compassresearch.ide.rttmbt.RttMbtPropertyTester"
            namespace="eu.compassresearch.ide.rttmbt.RttMbtPropertyTester"
            properties="isRttMbtProject, isTestGenerationContext, isRttTestprocedure"
            type="java.lang.Object">
      </propertyTester>
   </extension>
